<h1>キューポイント登録</h1>

<div class="row">
  <div class="col-xs-6">
    <%= form_for(@cuepoint) do |f| %>
      <% render 'shared/error_messages', object: f.object %>
      <div class="form-group">
        <%= f.label :name, 'name' %>
        <%= f.text_field :name, class: 'form-control' %>
      </div>

      <%= f.submit '登録', class: 'btn btn-primary' %>
      
    <% end %>  
  </div>
</div>